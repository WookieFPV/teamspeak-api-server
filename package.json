{
  "name": "bun-hono-server",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "run": "bun run src/index.ts",
    "format": "bunx biome format --write .",
    "lint": "bunx biome lint --fix",
    "lint:CI": "bunx biome check",
    "check": "bunx biome check --fix"
  },
  "dependencies": {
    "hono": "4.10.4",
    "p-queue": "9.0.0",
    "ts3-nodejs-library": "3.5.1",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@types/bun": "latest",
    "lefthook": "2.0.2",
    "typescript": "5.9.3"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
